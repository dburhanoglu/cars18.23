    <title>javascript: calculate two numbers</title>
    <meta charset="windows-1252">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script>
        function calc() {
            var ht = parseFloat(document.getElementById('ht').value);
            var wt = parseFloat(document.getElementById('wt').value);
            var bs = parseFloat(document.getElementById('bs').value);
            var ws = parseFloat(document.getElementById('ws').value);
            var hs = parseFloat(document.getElementById('hs').value);
            var hhs = parseFloat(document.getElementById('hhs').value);
            var oper = document.getElementById('operators').value;

            if (oper === 'calculate'){
              
               if (((hs - bs) > 2) && ((hs - ws) >= 7) && ((hhs / ws) >= 1.193)){
                   document.getElementById('result').value = 'spoon'; 
                    location.href = '/spoon.html';
                }
                    
                else if ((hs - bs) >= 3.6 && (bs - ws) < 9){
                     document.getElementById('result').value = 'triangle';
                      location.href = '/triangle.html';
                }
                   
                else if (((bs - hs) >= 3.6) && ((bs - ws) > 9)){
                         document.getElementById('result').value = ' inverted triangle';
                          location.href = '/invtriangle.html';

                }
               
                else if ((hs - bs) < 3.6 && (bs - hs) < 3.6 && (bs - ws) < 9 && (hs - ws) < 10){
                    document.getElementById('result').value = ' Rectangle';
                     location.href = '/rectangular.html';
                }

                     else if ((bs - hs) <= 1 && (hs - bs) < 3.6 && ((bs - ws) <= 9 || (hs - ws) >= 10)||((hs - bs) >= 3.6 && (hs - bs) <= 10 && (hs - ws) >= 9 && (hhs / ws) > 1.193)||((bs - hs) > 1 && (bs - hs) < 10 && (bs - ws) >= 9))
                {
                    document.getElementById('result').value = 'Hourglass';
                    
                    location.href = '/selena.html';
                }
                else
                    document.getElementById('result').value = 'out of category';
            }
        }
    </script>
    
    
        <input type="number" id="ht" /><br /><br />
        <input type="number" id="wt" /><br /><br />
        <input type="number" id="bs" /><br /><br />
        <input type="number" id="ws" /><br /><br />
        <input type="number" id="hs" /><br /><br />
        <input type="number" id="hhs" /><br /><br />
        <select id="operators">
            <option value="calculate">calculate</option>
        </select>
        <button onclick="calc();">=</button>
        <input type="text" id="result" />
   
    
